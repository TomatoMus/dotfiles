{
  "ecmaVersion": 6,
  "libs": [
    "browser",
    "ecma5",
    "ecma6",
    "ecma7",
    "jquery"
    ],
    "loadEagerly": [
      "importantfile.js",
      "node_modules/react-dom/**/*.js",
      "node_modules/react/**/*.js",
      "node_modules/whatwg-fetch/fetch.js",
      "src/**/*.js"
    ],
    "plugins": {
      "es_modules": {},
      "node": {},
      "requirejs": {
        "baseURL": "./",
        "paths": {}
      },
      "doc_comment": {
        "fullDocs": true,
        "strong": true
      }
    }
}
